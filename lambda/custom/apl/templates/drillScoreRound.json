{
    "type": "APL",
    "version": "1.1",
    "theme": "dark",
    "import": [
        {
            "name": "alexa-layouts",
            "version": "1.1.0"
        }
    ],
    "resources": [],
    "styles": {},
    "layouts": {},
    "mainTemplate": {
        "parameters": [
            "payload"
        ],
        "items": [
            {
                "type": "Container",
                "width": "100%",
                "height": "100%",
                "items": [
                    {
                        "type": "AlexaBackground",
                        "backgroundImageSource": "https://circle-of-fifths.s3-eu-west-1.amazonaws.com/cof2.png",
                        "coverOverlay": true
                    },
                    {
                        "type": "AlexaHeader",
                        "headerBackButton": true,
                        "headerBackButtonAccessibilityLabel": "back",
                        "headerBackgroundColor": "transparent",
                        "headerTitle": "Header title",
                        "headerSubtitle": "Header subtitle",
                        "headerAttributionText": "Attribution",
                        "headerAttributionImage": "https://d2o906d8ln7ui1.cloudfront.net/images/cheeseskillicon.png",
                        "headerAttributionPrimacy": true,
                        "headerDivider": false
                    },
                    {
                        "type": "Image",
                        "width": "350",
                        "height": 60,
                        "source": "https://circle-of-fifths.s3-eu-west-1.amazonaws.com/button_order-of-sharps-flats-96.png",
                        "alignSelf": "center"
                    },
                    {
                        "type": "Text",
                        "textAlign": "center",
                        "color": "#9900ff",
                        "fontFamily": "serif, times new roman, times, georgia",
                        "text": "Level 2",
                        "fontWeight": "600",
                        "top": "40"
                    },
                    {
                        "type": "Text",
                        "textAlign": "center",
                        "color": "#9900ff",
                        "text": "Question 5",
                        "fontFamily": "serif, times new roman, times, georgia",
                        "top": "60"
                    },
                    {
                        "type": "Text",
                        "textAlign": "center",
                        "color": "#9900ff",
                        "text": "x/y",
                        "fontFamily": "serif, times new roman, times, georgia",
                        "top": "80"
                    }
                ]
            }
        ]
    }
}