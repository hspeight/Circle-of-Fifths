{
    "type": "APL",
    "version": "1.1",
    "theme": "dark",
    "import": [
        {
            "name": "alexa-layouts",
            "version": "1.1.0"
        }
    ],
    "resources": [],
    "styles": {},
    "layouts": {},
    "mainTemplate": {
        "parameters": [
            "payload"
        ],
        "items": [
            {
                "type": "Container",
                "width": "100%",
                "height": "100%",
                "alignItems": "center",
                "justifyContent": "center",
                "items": [
                    {
                        "type": "Text",
                        "width": "300dp",
                        "height": "32",
                        "textAlign": "center",
                        "textAlignVertical": "auto",
                        "color": "yellow",
                        "fontStyle": "italic",
                        "fontSize": "24dp",
                        "text": "${payload.unknownProductData.text.title}",
                        "position": "relative",
                        "top": "-30"
                    },
                    {
                        "type": "Image",
                        "width": "60%",
                        "height": "60%",
                        "source": "https://circle-of-fifths.s3-eu-west-1.amazonaws.com/COF.png",
                        "bottom": "20"
                    }
                ]
            }
        ]
    }
}