{
    "type": "APL",
    "version": "1.1",
    "theme": "dark",
    "import": [
        {
            "name": "alexa-layouts",
            "version": "1.1.0"
        }
    ],
    "resources": [],
    "styles": {},
    "layouts": {},
    "mainTemplate": {
        "parameters": [
            "payload"
        ],
        "items": [
            {
                "type": "Container",
                "width": "100%",
                "height": "100%",
                "alignItems": "center",
                "justifyContent": "center",
                "items": [
                    {
                        "type": "Image",
                        "width": "75%",
                        "height": "75%",
                        "source": "https://circle-of-fifths.s3-eu-west-1.amazonaws.com/COF.png",
                        "align": "center",
                        "alignSelf": "center",
                        "position": "relative"
                    },
                    {
                        "type": "Frame",
                        "width": "70%",
                        "height": "15%",
                        "alignSelf": "center",
                        "position": "absolute",
                        "item": {
                            "type": "AlexaFooter",
                            "hintText": "${payload.unknownProductData.text.title}"
                        },
                        "bottom": "0"
                    }
                ]
            }
        ]
    }
}